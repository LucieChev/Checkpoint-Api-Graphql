{
    "compilerOptions": {
      "target": "ES2018",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true,
      "outDir": "./dist"
    },
    "include": [
      "src/**/*.ts"
    ]
  }
  